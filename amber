function toggleMobileNav(){document.getElementById('mobileNav').classList.toggle('active');}
function setLanguage(l){lang=l;document.querySelectorAll('.lang-btn').forEach(b=>b.classList.toggle('active',b.textContent===l.toUpperCase()));document.querySelectorAll('[data-i18n]').forEach(el=>{const k=el.getAttribute('data-i18n');if(T[l]&&T[l][k])el.textContent=T[l][k];});document.documentElement.dir=l==='ar'?'rtl':'ltr';document.documentElement.lang=l;renderPortfolio();renderCollections();renderTiles();}
function renderPortfolio(){const g=document.getElementById('portfolioGrid');g.innerHTML=portfolioData.map(p=>`<div class="portfolio-item"><div class="portfolio-image">${p.svg}</div><div class="portfolio-content"><h3 data-i18n="portfolio_${p.key}">${T[lang]['portfolio_'+p.key]||p.key}</h3><p data-i18n="portfolio_${p.key}_desc">${T[lang]['portfolio_'+p.key+'_desc']||''}</p></div></div>`).join('');}
function renderCollections(){const g=document.getElementById('collectionsGrid');g.innerHTML=collectionsData.map(c=>`<div class="collection-card" onclick="navigateTo('tiles')"><div class="collection-image">${c.svg}</div><div class="collection-info"><h3 data-i18n="collection_${c.key}">${T[lang]['collection_'+c.key]||c.key}</h3><p data-i18n="collection_${c.key}_desc">${T[lang]['collection_'+c.key+'_desc']||''}</p></div></div>`).join('');}
function renderTiles(){const g=document.getElementById('tilesGrid');const f=getFilters();const filtered=tiles.filter(t=>{for(const[k,v]of Object.entries(f))if(v.length&&!v.includes(t[k]))return false;return true;});if(!filtered.length){g.innerHTML=`<div class="no-results"><h4 data-i18n="no_results">${T[lang].no_results||'No products found'}</h4><p data-i18n="no_results_desc">${T[lang].no_results_desc||'Try adjusting your filters'}</p></div>`;return;}g.innerHTML=filtered.map(t=>`<div class="tile-card"><div class="tile-image">${genTileSVG(t.color,t.transparency)}</div><div class="tile-info"><h4>${t.name}</h4><div class="tile-meta"><span class="tile-tag">${t.collection}</span><span class="tile-tag">${t.size}</span></div></div></div>`).join('');}
function getFilters(){const f={};document.querySelectorAll('.filter-options input[type="checkbox"]:checked').forEach(c=>{const g=c.closest('.filter-options').getAttribute('data-filter'),v=c.getAttribute('data-filter-value');if(!f[g])f[g]=[];f[g].push(v);});return f;}
function applyFilters(){renderTiles();}
function resetFilters(){document.querySelectorAll('.filter-options input[type="checkbox"]').forEach(c=>c.checked=false);renderTiles();}
function renderBlogs(){const g=document.getElementById('blogsGrid');g.innerHTML=blogsData.map((b,i)=>`<div class="blog-card"><div class="blog-image">${genBlogSVG(i)}</div><div class="blog-content"><div class="blog-date">${new Date(b.date).toLocaleDateString('en-GB')}</div><div class="blog-title">${b.title}</div></div></div>`).join('');}
function toggleAccordion(h){const a=h.classList.contains('active');document.querySelectorAll('.accordion-header').forEach(x=>{x.classList.remove('active');x.nextElementSibling.classList.remove('active');});if(!a){h.classList.add('active');h.nextElementSibling.classList.add('active');}}
function handleFormSubmit(e){e.preventDefault();alert('Thank you! Your request has been sent.');e.target.reset();}
function handleContactSubmit(e){e.preventDefault();alert('Thank you! We will contact you shortly.');e.target.reset();}
function handlePagination(n){document.querySelectorAll('.pagination-btn').forEach(b=>b.classList.remove('active'));event.target.classList.add('active');}
</script>
</body>
</html>
